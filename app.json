{
  "expo": {
    "name": "Animations",
    "slug": "Animations",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/logo.png",
    "scheme": "animations",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/images/A.png",
      "resizeMode": "cover",
      "backgroundColor": "#FA4268"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.afridi.expoanimations"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/logo.png",
        "backgroundColor": "#FA4268"
      },
      "edgeToEdgeEnabled": true,
      "package": "dev.afridi.expoanimations"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./app/screens"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "root": "./app/screens"
      },
      "eas": {
        "projectId": "3aec64c7-3a45-4749-843c-d7fbc0c808dc"
      }
    }
  }
}
